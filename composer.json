{
  "name": "fulll/onesky-bundle",
  "description": "One Sky Bundle for PHP",
  "keywords": ["php", "translation", "onesky", "bundle", "symfony"],
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Romain Kuzniak",
      "email": "romain.kuzniak@openclassrooms.com"
    },
    {
      "name": "OpenClassrooms",
      "email": "tech@openclassrooms.com"
    },
    {
      "name": "Fulll Team",
      "homepage": "https://github.com/fulll/onesky-bundle/graphs/contributors"
    }
  ],
  "autoload": {
    "psr-4": {
      "OpenClassrooms\\Bundle\\OneSkyBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OpenClassrooms\\Bundle\\OneSkyBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1",
    "onesky/api-library-php5": "^1.0",
    "symfony/dependency-injection": ">=2.8",
    "symfony/console": ">=2.8",
    "symfony/config": ">=2.8",
    "symfony/finder": ">=2.8",
    "symfony/framework-bundle": ">=2.8",
    "symfony/http-kernel": ">=2.8",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5.0",
    "satooshi/php-coveralls": "^2.2"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
